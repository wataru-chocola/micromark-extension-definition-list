{
  "packageManager": "pnpm@8.8.0",
  "name": "micromark-extension-definition-list",
  "version": "1.5.0",
  "description": "micromark extension to support definition lists",
  "author": "Wataru Watanabe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wataru-chocola/micromark-extension-definition-list.git"
  },
  "bugs": {
    "url": "https://github.com/wataru-chocola/micromark-extension-definition-list/issues"
  },
  "homepage": "https://github.com/wataru-chocola/micromark-extension-definition-list",
  "main": "./index.js",
  "types": "./index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dev/**/*.js",
    "dev/**/*.d.ts",
    "!dev/**/*.test.js",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.js",
    "index.d.ts",
    "index.js"
  ],
  "exports": {
    "development": "./dev/index.js",
    "default": "./index.js"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "micromark-core-commonmark": "^2.0.0",
    "micromark-factory-space": "^1.0.0",
    "micromark-util-character": "^2.0.1",
    "micromark-util-chunked": "^1.0.0",
    "micromark-util-symbol": "^1.0.0",
    "micromark-util-types": "^1.1.0",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@types/assert": "^1.5.7",
    "@types/debug": "^4.1.9",
    "@types/jest": "29.5.4",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "debug": "4.3.4",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "jest": "29.6.4",
    "micromark": "3.1.0",
    "micromark-build": "1.3.0",
    "micromark-extension-gfm": "2.0.3",
    "micromark-extension-gfm-table": "1.0.7",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "micromark": "^4.0.0"
  },
  "keywords": [
    "micromark",
    "micromark-extension"
  ],
  "scripts": {
    "format": "prettier --write \"./dev/**/*.{js,ts,json}\"",
    "lint": "eslint ./dev/",
    "lint-fix": "eslint --fix ./dev/**/*.{ts} && prettier --write ./dev/**/*.{ts}",
    "test": "npm run build && NODE_OPTIONS=--experimental-vm-modules jest --testPathIgnorePatterns=\"/node_modules/|/dev/\"",
    "test-dev": "tsc && NODE_OPTIONS=\"--experimental-vm-modules -C development\" jest \"dev/.*\\.test\\.js\"",
    "build": "npm run clean && tsc && micromark-build",
    "prepack": "npm run build",
    "clean": "rimraf lib/ index.js index.test.js dev/**/*.js"
  }
}
